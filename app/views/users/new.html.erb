<%= form_for @user do |f|  %>

<div>
  <%= f.label :name %><br/>
  <%= f.text_field :name %></div>
<div>
  <%= f.label :email %><br/>
  <%= f.email_field :email %> </div>
<div>
  <%= f.label :password %><br/>
  <%= f.password_field :password %> </div>
<div>
  <%= f.label :handle %><br/>
  <%= f.text_field :handle %></div>
<%= f.submit "Become funky"  %>
<% end  %> 